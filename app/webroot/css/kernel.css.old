@charset "utf-8";

* {
	padding: 0px;
	margin: 0px;
}

html {
	height: 100%;
}

body {
	height: 100%;
	font-family: Helvetica, Arial, sans-serif;
	font-size: 12px;
	color: #333;
	background: #fff;
}

div.clear {
	height: 0px;
	overflow: hidden;
	clear: both;
}

.bold {
	font-weight: bold;
}

.icon {
	padding: 3px 0px 1px 20px;
	background: no-repeat center left;
}

a {
	text-decoration: underline;
}

a:hover {
	text-decoration: none;
}

/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * 
 * - Header
 * 
 * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

div#header {
	width: 100%;
}

	div#header a {
		font-weight: bold;
	}

	div#header div.top {
		width: 100%;
		height: auto !important;
		height: 100px;
		min-height: 100px;
		background: #333;
		padding-bottom: 3px;
	}
	
		div#header div.top div.logo {
			width: 200px;
			height: 100px;
			float: left;
			background: no-repeat center;
			margin-right: 10px;
		}
		
		div#header div.top p {
			color: #fff;
			line-height: 35px;
			font-weight: bold;
			text-indent: 4px;
		}
		
			div#header div.top p span {
				color: #ccc;
				display: inline-block;
			}
		
			div#header div.top p span.admin {
				background-image: url(../img/sistema/icon_admin.png);
			}
			
			div#header div.top p span.date {
				background-image: url(../img/sistema/icon_date.png);
			}
			
			div#header div.top p a.sair {
				background-image: url(../img/sistema/icon_sair.png);
				color: #fff;
				float: right;
				margin-right: 20px;
			}
		
		div#header div.top ul#menu {
			padding-top: 29px;
		}
		
			div#header div.top ul#menu li {
				display: block;
				float: left;
				height: 26px;
				line-height: 26px;
				padding: 3px;
			}
			
				div#header div.top ul#menu li a {
					display: block;
					height: 26px;
					padding: 0px 12px;
					background: #666;
					color: #fff;
					font-family: "Lucida Grande", 'Trebuchet MS', Helvetica, Arial, sans-serif;
					text-decoration: none;
					font-size: 14px;
				}
				
				div#header div.top ul#menu li a:hover {
					background: #555;
				}
				
				div#header div.top ul#menu li a.selected,
				div#header div.top ul#menu li a.selected:hover {
					height: 24px;
					line-height: 24px;
					border: 1px solid #999;
					background: #222;
				}
	
	div#header div.bottom {
		width: 100%;
		height: 50px;
		background: #eee;
	}
	
		div#header div.bottom div.left {
			float: left;
			width: 200px;
			height: 100%;
			border-right: 1px solid #fff;
			line-height: 50px;
			text-align: center;
		}
		
			div#header div.bottom div.left a {
				color: #333;
			}
		
			div#header div.bottom div.left a.home {
				background-image: url(../img/sistema/icon_home.png);
			}
			
			div#header div.bottom div.left a.dados {
				background-image: url(../img/sistema/icon_dados.png);
				margin-left: 5px;
			}
		
		div#header div.bottom h1 {
			font-size: 25px;
			line-height: 50px;
			text-indent: 10px;
			font-weight: normal;
		}
		
			div#header div.bottom h1 a {
				color: #333;
				font-weight: normal;
			}
			
			div#header div.bottom h1 .next {
				background: url(../img/sistema/seta.png) no-repeat 15px center;
				padding-left: 38px;
			}
			
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * 
 * - Middle
 * 
 * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

div#middle {
	width: 100%;
}

	div#middle a {
		color: #333;
		font-weight: bold;
	}
	
	div#middle .Users {
		background-image: url(../img/sistema/icon_user.png);
	}
	
	div#middle .Profiles {
		background-image: url(../img/sistema/icon_profile.png);
	}
	
	div#middle .add {
		background-image: url(../img/sistema/icon_add.png);
	}
	
	div#middle .view {
		background-image: url(../img/sistema/icon_view.png);
	}
	
	div#middle .edit {
		background-image: url(../img/sistema/icon_edit.png);
	}
	
	div#middle .delete {
		background-image: url(../img/sistema/icon_delete.png);
	}
	
	div#middle .delete2 {
		background-image: url(../img/sistema/icon_delete_2.png);
	}

	div#middle div#submenu {
		width: 200px;
		height: auto !important;
		height: 400px;
		min-height: 400px;
		float: left;
		background: #eee;
		border-top: 1px solid #fff;
		padding-bottom: 50px;
	}
	
		div#middle div#submenu ul {
			list-style: none;
			width: 100%;
		}
		
			div#middle div#submenu ul li a {
				width: 160px;
				height: 48px;
				display: block;
				background-color: #ddd ;
				background-position: 20px center;
				line-height: 48px;
				padding-left: 40px;
				text-decoration: none;
			}
			
			div#middle div#submenu ul li a:hover {
				background-color: #ccc;
				text-decoration: underline;
			}
			
				div#middle div#submenu ul li ul li a {
					height: 30px;
					line-height: 30px;
					background-color: #eee;
					background-position: 30px center;
					border-bottom: 1px solid #ddd;
					padding-left: 50px;
					width: 150px;
				}
	
	div#middle div#content {
		padding: 15px 13px;
		margin-left: 202px;
	}
	
		div#middle div#content div.div {
			height: 0px;
			overflow: hidden;
			border-top: 1px solid #ddd;
			margin: 10px 0px;
		}
		
		div#middle div#content div.div.medium {
			width: 760px;
		}
		
		div#middle div#content fieldset {
			border: 1px solid #ddd;
			padding: 2px 5px 5px 5px;
		}
		
		div#middle div#content fieldset.medium {
			width: 750px;
		}
		
			div#middle div#content fieldset legend {
				padding: 0px 5px;
				font-size: 14px;
				font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
			}
	
		/*---------------- LISTAGENS -----------------------------------*/
	
		div#middle div#content table.list {
			width: 100%;
			background: #f1f1f1;
			text-align: left;
			border-collapse: collapse;
			border-spacing: 0px;
		}
		
		div#middle div#content table.list.auto {
			width: auto;
		}
		
		div#middle div#content table.list.medium {
			width: 760px;
		}
		
			div#middle div#content table.list tr {
				height: 28px;
			}
			
			div#middle div#content table.list tr.head {
				height: 36px;
				background: #666;
				color: #fff;
				font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
			}
			
				div#middle div#content table.list tr.head a {
					color: #fff;
				}
			
			div#middle div#content table.list tr.altrow {
				background: #fff;
			}
			
			div#middle div#content table.list tr.footer {
				background: #ccc;
				height: 36px;
			}
			
				div#middle div#content table.list tr.footer td.right {
					text-align: right;
					padding-right: 10px;
				}
			
			div#middle div#content table.list th {
				padding-left: 4px;
			}
			
			div#middle div#content table.list th.check,
			div#middle div#content table.list td.check {
				padding-left: 0px;
				width: 30px;
				text-align: center;
			}
			
			div#middle div#content table.list th.actions {
				width: 220px;
			}
			
			div#middle div#content table.list td {
				padding-left: 4px;
			}
			
				div#middle div#content table.list td a.icon {
					margin-right: 6px;
				}
			
			div#middle div#content table.list td.pic {
				padding-left: 0px;
				width: 30px;
				background-position: center;
				background-repeat: no-repeat;
			}
			
		/*------------- FORMS ----------------------------------------------*/
	
		div#middle div#content form.actions input.submit {
			margin-bottom: 15px;
			border: 1px solid #ddd;
			background: #eee;
			color: #666;
			cursor: pointer;
			display: inline-block;
			line-height: 25px;
			height: 25px;
			font-weight: bold;
			font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
			padding: 0px 8px;
		}
		
		div#middle div#content form.actions input.submit:hover {
			background-color: #666;
			border-color: #666;
			color: #fff;
		}
		
		div#middle div#content form.form label {
			font-weight: bold;
			margin-top: 4px;
			display: block;
			font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
		}
		
			div#middle div#content form.form label span.desc {
				color: #666;
				font-size: 10px;
			}
			
			div#middle div#content form.form label span.req,
			div#middle div#content form.form p.required span {
				color: #ff0000;
				font-size: 14px;
				line-height: 12px;
				font-weight: bold;
			}
			
			div#middle div#content form.form p.required {
				color: #d80000;
				float: left;
				margin-top: 5px;
			}
		
		div#middle div#content form.form input.text {
			width: 250px;
			display: block;
			border: 1px solid #fff;
			background: #eee;
			font-size: 13px;
			font-family: Helvetica, Arial, sans-serif;
			padding: 6px 3px 6px 5px;
		}
		
		div#middle div#content form.form input.text.small {
			width: 117px;
		}
		
		div#middle div#content form.form input.text.inline {
			float: left;
		}
		
		div#middle div#content form.form input.text.left {
			margin-right: 6px;
		}
		
		div#middle div#content form.form textarea.textarea {
			width: 248px;
			display: block;
			border: 1px solid #fff;
			background: #eee;
			font-size: 13px;
			font-family: Helvetica, Arial, sans-serif;
			padding: 5px;
		}
		
		div#middle div#content form.form table.visualizar div.input.textarea table td {
			padding: 0px;
		}
		
		div#middle div#content form.form textarea.textarea.big {
			width: 500px;
		}
		
		div#middle div#content form.form textarea.textarea.high {
			height: 200px;
		}
		
		div#middle div#content form.form select.select {
			width: 260px;
			display: block;
			border: 1px solid #fff;
			background: #eee;
			font-size: 13px;
			font-family: Helvetica, Arial, sans-serif;
			padding: 5px 5px 5px 2px;
			outline: none;
		}
		
		div#middle div#content form.form select.select.small {
			width: 127px;
		}
		
		div#middle div#content form.form input.text:focus,
		div#middle div#content form.form textarea.textarea:focus,
		div#middle div#content form.form select.select:focus {
			background-color: #fff;
			border-color: #ddd;
		}
		
		div#middle div#content form.form input.uppercase {
			text-transform: uppercase;
		}
		
		div#middle div#content form.form input.submit {
			float: right;
			margin: 20px 4px 0px 0px;
			border: none;
			background: #007700;
			color: #fff;
			cursor: pointer;
			display: inline-block;
			line-height: 25px;
			height: 25px;
			font-weight: bold;
			font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
			padding: 0px 8px;
		}
		
		div#middle div#content form.form input.submit:hover {
			background: #005500;
		}
		
		div#middle div#content form.form input.submit.cancel {
			background: #999;
			margin-right: 8px;
		}
		
		div#middle div#content form.form input.submit.cancel:hover {
			background: #666;
		}
		
		div#middle div#content form.form input.submit.disabled,
		div#middle div#content form.form input.submit.disabled:hover {
			background: #999;
			color: #eee;
		}
		
		div#middle div#content form.form input.submit.spinner.on {
			background-image: url(../img/sistema/spinner.gif);
			background-position: center;
			background-repeat: no-repeat;
		}
		
		div#middle div#content form.form div.checkbox {
			
		}
		
			div#middle div#content form.form div.radio input,
			div#middle div#content form.form div.radio input.radio,
			div#middle div#content form.form div.checkbox input,
			div#middle div#content form.form div.checkbox input.checkbox {
				display: block;
				float: left;
				height: 20px;
				margin-right: 8px;
				margin-left: 5px;
			}
		
			div#middle div#content form.form div.checkbox label {
			
			}
			
		div#middle div#content form.form table.visualizar td.areas fieldset {
			margin-bottom: 10px;
		}
		
		div#middle div#content form.form table.visualizar td.input {
			width: 260px;
		}
		
		div#middle div#content form.form table.visualizar td.input.small {
			width: 119px;
		}
		
		div#middle div#content form.form table.visualizar a.icon {
			margin-top: 2px;
			display: inline-block;
		}
		
		div#middle div#content form.form .error-message {
			color: #d80000;
		}
		
		div#middle div#content #flashMessage {
			font-weight: bold;
			padding: 15px;
			margin-bottom: 10px;
		}
		
		div#middle div#content #flashMessage.error {
			color: #b80000;
			background: #FFE6E6;
		}
		
		div#middle div#content #flashMessage.success {
			color: #009900;
			background: #eeeee0;
		}
	
		/*------------ VISUALIZACAO DE DADOS -------------------------------*/
	
		div#middle div#content table.visualizar {
			line-height: 20px;
			border-collapse: collapse;
			border-spacing: 0px;
			width: 100%;
			margin-left: 6px;
		}
		
		div#middle div#content fieldset table.visualizar {
			margin-left: 0px;
		}
		
		div#middle div#content table.visualizar.medium {
			width: 754px;
		}
		
		div#middle div#content table.visualizar.auto {
			width: auto;
		}
		
			div#middle div#content table.visualizar tr.altrow {
				background: #f5f5f5;
			}
			
			div#middle div#content table.visualizar td {
				padding: 5px 6px;
				vertical-align: top;
			}
			
			div#middle div#content table.visualizar td.center {
				vertical-align: middle;
			}
		
			div#middle div#content table.visualizar td.label {
				font-weight: bold;
				padding-right: 10px;
				width: 170px;
				font-family: "Lucida Grande", Helvetica, Arial, sans-serif;
			}
			
			div#middle div#content table.visualizar td.label.small {
				width: 100px;
			}
			
			div#middle div#content table.visualizar td.label.medium {
				width: 130px;
			}
			
			div#middle div#content table.visualizar td.label.big {
				width: 230px;
			}
			
			div#middle div#content table.visualizar td.label.right {
				text-align: right;
			}
			
		div#middle div#content div.buttons {
			border-top: 1px solid #ccc;
			padding: 10px 2px;
			margin-top: 10px;
		}
		
		div#middle div#content div.buttons.top {
			border-top: none;
			border-bottom: 1px solid #ccc;
			padding: 0px 2px 10px 2px;
			margin-top: 0px;
			margin-bottom: 10px;
		}

		div#middle div#content div.buttons.middle {
			border-bottom: 1px solid #ccc;
			margin-bottom: 10px;
		}
		
		
		div#middle div#content a.button {
			display: inline-block;
			background-color: #666;
			font-weight: bold;
			color: #fff;
			padding: 0px 10px;
			height: 25px;
			line-height: 25px;
			margin: 0px 4px;
		}
		
		div#middle div#content a.button:hover {
			background-color: #333;
		}
		
		div#middle div#content a.button.icon {
			padding-left: 30px;
			background-position: 7px center;
		}
		
		div#middle div#content a.button.delete2 {
			background-color: #b90000;
		}
		
		div#middle div#content a.button.delete2:hover {
			background-color: #900000;
		}
		
		div#middle div#content a.button.voltar {
			background-color: #666;
			padding: 0px 7px;
			text-decoration: none;
			height: 18px;
			line-height: 18px;
			margin: 0px 0px 0px 5px;
			font-size: 10px;
			letter-spacing: 0.04em;
			text-transform: uppercase;
		}

		div#middle div#content a.button.voltar:hover {
			background-color: #333;
		}
		
		/*------------------------ SEARCH ---------------------------------*/
		
		div#content div.search {
			background: #fff;
			margin-bottom: 1px;
			padding: 10px;
			height: 30px;
		}

		div#content div.search.medium {
			width: 740px;
		}

			div#content div.search form.form label {
				float: left;
				padding-right: 5px;
				line-height: 18px;
			}

			div#middle div#content div.search form.form input.text {
				float: left;
				width: 170px;
			}

			div#middle div#content div.search form.form input.submit {
				float: left;
				margin: 2px 0px 0px 8px;
				background: #333;
			}

			div#middle div#content div.search form.form select.select {
				float: left;
				width: 170px;
				margin-left: 5px;
			}
		
		/*------------------------ PAGINACAO ------------------------------*/
		
		div#middle div#content div.pagination {
			height: 36px;
		}

			div#middle div#content div.pagination span,
			div#middle div#content div.pagination div {
				display: block;
				float: left;
				padding: 0px;
				line-height: 36px;
			}

			div#middle div#content div.pagination div {
				padding: 0px 20px;
			}
			
			div#middle div#content div.pagination div.total {
				float: right;
			}

			div#middle div#content div.pagination .prev-enabled {
				background: url(../img/sistema/icon_prev.png) no-repeat center left;
				padding-left: 18px;
			}

			div#middle div#content div.pagination .prev-disabled {
				background: url(../img/sistema/icon_prev_disabled.png) no-repeat center left;
				padding-left: 18px;
			}

			div#middle div#content div.pagination .next-enabled {
				background: url(../img/sistema/icon_next.png) no-repeat center right;
				padding-right: 18px;
			}

			div#middle div#content div.pagination .next-disabled {
				background: url(../img/sistema/icon_next_disabled.png) no-repeat center right;
				padding-right: 18px;
			}
			
/*------------------------ FOOTER ----------------------------------------*/
		
div#footer {
	background: #666;
	padding: 15px;
	margin-top: 50px;
}

	div#footer p {
		color: #fff;
	}
	
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 * 
 * - CAKE SQL TABLE
 * 
 * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

table.cake-sql-log {
	background: #f4f4f4;
	border:1px solid #ccc;
	border-right:0;
	clear: both;
	color: #333;
	margin-bottom: 10px;
	width: 100%;
}

table.cake-sql-log th {
	background: #f2f2f2;
	border:1px solid #bbb;
	border-top: 1px solid #fff;
	border-left: 1px solid #fff;
	text-align: center;
}

table.cake-sql-log th a {
	background:#f2f2f2;
	display: block;
	padding: 2px 4px;
	text-decoration: none;
}

table.cake-sql-log th a:hover {
	background: #ccc;
	color: #333;
	text-decoration: none;
}

table.cake-sql-log tr td {
	background: #fff;
	border-right: 1px solid #ccc;
	/*padding: 4px;*/
	text-align: center;
	vertical-align: top;
	padding: 4px 8px;
	text-align: left;
}

table.cake-sql-log tr.altrow td {
	background: #f4f4f4;
}

table.cake-sql-log td.actions {
	text-align: center;
	white-space: nowrap;
}

table.cake-sql-log td.actions a {
	margin: 0px 6px;
}
